{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "outDir": "built",
    "rootDir": "src",
    "sourceMap": false
  },
  "files": [
    "typings/main.d.ts",
    "src/main.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "typhen": [
    {
      "plugin": "typhen-json-schema",
      "pluginOptions": {
        "enumType": "string"
      },
      "outDir": "built/generated/schemas",
      "files": [ "src/typings/json.d.ts" ]
    }
  ]
}
